;
; BIND data file for local loopback interface
;
$TTL	60
@	IN	SOA	dnet. root.dnet. (
			      15		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 5 )	; Negative Cache TTL
;
@	IN	NS	dnet.
@	IN	AAAA	::1
@	IN	A	172.16.1.8
@	IN	MX	10 mail.dnet.
satu	IN	CNAME	dnet.


;one.dnet.	IN CNAME	dnet.
;mail	IN	A	172.16.1.8


;mail.one.dnet.	IN	A	172.16.1.8
;mail.two.dnet.	IN	A	172.16.1.8
;one.dnet. IN	MX	10 mail.one.dnet.
;two.dnet. IN	MX	10 mail.two.dnet.
